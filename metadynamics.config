[SYSTEM]
nonbondedMethod = PME
nonbondedCutoff = 1.1
ewaldErrorTolerance = 0.00001
constraints = h-bonds
rigidWater = True
constraintTolerance = 0.0001

[INTEGRATOR]
dt = 0.002
temperature = 300
friction = 1.0
pressure = 1.0
barostatInterval = 25

[SIMULATION]
steps = 20000000
equilibrationSteps = 100000

[PLATFORM]
platform = CUDA
Precision = mixed