[UNITS]
size_unit = nanometers
time_unit = picoseconds
temp_unit = kelvin
press_unit = bar

[SYSTEM]
nonbondedMethod = PME
nonbondedCutoff = 1.1
ewaldErrorTolerance = 0.00001
constraints = HBonds
rigidWater = True
constraintTolerance = 0.0001

[INTEGRATOR]
dt = 0.002
temperature = 300
friction = 5.0
pressure = 1.0
barostatInterval = 25

[SIMULATION]
steps = 20000000
equilibrationSteps = 100000
reportInterval = 50
currentStep = 0

[PLATFORM]
platform = CUDA

[PLATFORM_PROPERTIES]
Precision = mixed
