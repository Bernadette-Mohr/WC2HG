[UNITS]
size_unit = nanometers
time_unit = picoseconds
temp_unit = kelvin
press_unit = bar

[SYSTEM]
nonbondedMethod = PME
nonbondedCutoff = 1.1
ewaldErrorTolerance = 0.00001
constraints = HBonds
rigidWater = True
constraintTolerance = 0.0001

[INTEGRATOR]
dt = 0.002
temperature = 300
friction = 1.0
pressure = 1.0
barostatInterval = 25

[PLATFORM]
platform = GPU

[PLATFORM_PROPERTIES]
Precision = mixed

[OPS_OPTIONS]
n_steps_per_frame = 50
n_frames_max = 50000